      "headers": {
        "Authorization": "Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiJ9..E3N1uk0QP7c1Gn86ItQw4Jg5hPUurLv2ob6Y__kEyVHikdVcEJVddocCReX1ozCyr-JgMADCDMa0jn9iLoNPWM30lPJfRo2MnqOqMekzmOTcRf2Ns5tpp_zsJdziKgvLjLtb1HhhM44nzS3DpeG-abhr-t1ALlavqDt45Um9x2Wbe5qmaaRM4xv5x4FGgLhUyetSRan5rwC5ImHHe2DRyfJTjwOlUZYIVxdcUIvhH-mt7oVtiz9ppQGHEyXuRpJYwSkoXhrbTRNGbOBEegaEL0iH6wNrw5NbRhwfuThOwMn09Vm26bwYGR6FBoi-aTfu1_s4G2RYYjNf3szbaSzCUA",
        "X-Tenant-Id": "11",
        "Accept": "*/*",
        "Content-Type": "application/json"
      },
      "parameters": [
        {
          "name": "PageID",
          "type": "number",
          "description": "Page number for pagination",
          "required": true
        },
        {
          "name": "PageSize",
          "type": "number",
          "description": "Number of items per page",
          "required": true
        },
        {
          "name": "Filters",
          "type": "string",
          "description": "Filter type (e.g. KubeArmor)",
          "required": true
        },
        {
          "name": "Search",
          "type": "string",
          "description": "Search query string",
          "required": false
        },
        {
          "name": "SortBy",
          "type": "string",
          "description": "Field to sort results by",
          "required": false
        }
      ],
      "timeout": "30s",
      "retries": 2
    },
    {
      "name": "add_filter",
      "description": "Save or update a new alert filter",
      "method": "POST",
      "endpoint": "https://cwpp.dev.accuknox.com/integrations/v3/filters",
      "body_template": "{\"FilterName\":\"{{.FilterName}}\",\"FilterQuery\":\"{{.FilterQuery}}\",\"Filters\":\"{{.Filters}}\",\"Option\":\"{{.Option}}\"{{if .FilterID}},\"FilterID\":{{.FilterID}}{{end}}}",
      "headers": {
        "Authorization": "Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiJ9..E3N1uk0QP7c1Gn86ItQw4Jg5hPUurLv2ob6Y__kEyVHikdVcEJVddocCReX1ozCyr-JgMADCDMa0jn9iLoNPWM30lPJfRo2MnqOqMekzmOTcRf2Ns5tpp_zsJdziKgvLjLtb1HhhM44nzS3DpeG-abhr-t1ALlavqDt45Um9x2Wbe5qmaaRM4xv5x4FGgLhUyetSRan5rwC5ImHHe2DRyfJTjwOlUZYIVxdcUIvhH-mt7oVtiz9ppQGHEyXuRpJYwSkoXhrbTRNGbOBEegaEL0iH6wNrw5NbRhwfuThOwMn09Vm26bwYGR6FBoi-aTfu1_s4G2RYYjNf3szbaSzCUA",
        "X-Tenant-Id": "11",
        "Accept": "*/*",
        "Content-Type": "application/json"
      },
      "parameters": [
        {
          "name": "FilterName",
          "type": "string",
          "description": "Name of the filter",
          "required": true
        },
        {
          "name": "FilterQuery",
          "type": "string",
          "description": "Filter query string",
          "required": true
        },
        {
          "name": "Filters",
          "type": "string",
          "description": "Filter type (e.g. KubeArmor)",
          "required": true
        },
        {
          "name": "Option",
          "type": "string",
          "description": "Operation type (save or update)",
          "required": true,
          "enum": ["save", "update"]
        },
        {
          "name": "FilterID",
          "type": "number",
          "description": "Filter ID (required for update operation)",
          "required": false
        }
      ],
      "timeout": "30s",
      "retries": 2
    }
  ],
  "prompts": [],
  "resources": [],
  "security": {
    "rate_limit": 100,
    "allowed_origins": ["https://app.dev.accuknox.com"],
    "allowed_headers": ["Authorization", "X-Tenant-Id", "Content-Type"]
  }
}